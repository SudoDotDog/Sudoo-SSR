{
    "compileOnSave": false,
    "compilerOptions": {
        "alwaysStrict": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2016",
            "es2017"
        ],
        "module": "CommonJS",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "removeComments": true,
        "resolveJsonModule": true,
        "strict": true,
        "strictNullChecks": true,
        "target": "es6"
    },
    "exclude": [],
    "include": []
}